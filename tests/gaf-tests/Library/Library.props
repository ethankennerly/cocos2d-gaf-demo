<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <CCX_ROOT>$(MSBuildThisFileDirectory)..\..\..\</CCX_ROOT>
    <CCX_ROOT_2>$(MSBuildThisFileDirectory)..\..\cocos2d-x\</CCX_ROOT_2>
    <GAF_SOURCES_ROOT>$(MSBuildThisFileDirectory)Sources\</GAF_SOURCES_ROOT>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(GAF_SOURCES_ROOT);$(CCX_ROOT)/cocos;$(CCX_ROOT_2)/cocos;$(CCX_ROOT)/cocos/base;$(CCX_ROOT_2)/cocos/base;$(CCX_ROOT)/external/chipmunk/include/chipmunk;$(CCX_ROOT_2)/external/chipmunk/include/chipmunk;$(CCX_ROOT)/external/win32-specific/gles/include/OGLES;$(CCX_ROOT_2)/external/win32-specific/gles/include/OGLES;$(CCX_ROOT)/external/glfw3/include/win32/;$(CCX_ROOT_2)/external/glfw3/include/win32/;$(CCX_ROOT)/external/win32-specific/zlib/include;$(CCX_ROOT_2)/external/win32-specific/zlib/include;$(CCX_ROOT)/../bindings/manual;$(CCX_ROOT_2)/../bindings/manual;$(CCX_ROOT)/../external/spidermonkey/include/win32;$(CCX_ROOT_2)/../external/spidermonkey/include/win32;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="CCX_ROOT">
      <Value>$(CCX_ROOT)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="CCX_ROOT_2">
      <Value>$(CCX_ROOT_2)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="GAF_SOURCES_ROOT">
      <Value>$(GAF_SOURCES_ROOT)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>